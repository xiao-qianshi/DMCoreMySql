                selectID = data.F_Id;
                $("span.badge").html(data.F_GroupName + data.F_DialylisBedNo);
                resetForm();
            }
        });

        $("#btn_search").click(function () {
            var startDate = $("#txt_StartDate").val();
            var endDate = $("#txt_EndDate").val();
            $gridList.jqGrid('setGridParam', {
