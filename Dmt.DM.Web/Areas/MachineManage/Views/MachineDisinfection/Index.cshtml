            sortname: 'F_VisitDate desc',
            viewrecords: true,
            postData: {
                keyword:
                    JSON.stringify({
                        startDate: $("#txt_StartDate").val(),
                        endDate: $("#txt_EndDate").val()
                    })
            },
            onCellSelect: function (rowid, iCol, cellcontent, e) { //单击选择行
